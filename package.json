{
  "name": "node",
  "version": "1.0.0",
  "description": "Painel de Vendas",
  "main": "index.js",
  "productName": "M2",
  "scripts": {
    "w": "webpack --config webpack.common.js --watch",
    "start": "ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "start2": "electron .",
    "build": "webpack --mode=production --config webpack.common.js --env MODE=production",
    "compile": "electron-webpack",
    "package-osx": "electron-packager . $npm_package_productName --platform=darwin  --arch=x64 --out=release-builds --overwrite --icon=./src/assets/mls.icns",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-serialport --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",
    "package-linux": "electron-packager . electron-serialport --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "start_": "electron ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@apollo/client": "^3.7.3",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "electron": "^22.0.0",
    "electron-builder": "^24.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^17.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.1.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "html-webpack-plugin": "^5.5.0",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "request": "^2.88.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "urql": "^3.0.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "jwt-decode": "^3.1.2"
  }
}
